<%= link_to patient_path(patient), class: "text-decoration-none text-black", data: { turbo_frame: "patient_details" } do %>
  <p>
    <strong>Name:</strong> <%= patient.fullname %><br>
    <strong>Age:</strong> <%= patient.age %><br>
    <strong>Phone:</strong> <%= patient.phone_number %>
  </p>
<% end %>
